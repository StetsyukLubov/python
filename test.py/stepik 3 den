# # m = input("")  # вводим количество минут
# # a = int(m)  # переводим колличество минут из строчного в целочисленное
# # b = a // 60  # считаем целочисленное колличество часов
# # c = a % 60  # считаем целочисленное колличество оставшихся минут
# # b = str(b)  # переводим количество часов в переменную со строчным значением
# # c = str(c)  # переводим количество оставшихся в переменную со строчным значением
# # print(m + " мин - это " + b + " час " + c + " минут.")  # выводим результат

n = int(input())
print(n, end=' ')
print('мин - это',end=' ') 
print(n // int(60),end=' ')
print('час', n % 60, 'минут.')
m = int(input())
h = m // 60
m1 = m % 60

print (m, 'мин - это', h, 'час', m1, 'минут.' )

